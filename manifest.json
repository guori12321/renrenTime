{
    "manifest_version": 2,

    "name": "RenRen Time",

    "description": "This extension demonstrates a 'browser action' with kittens.",

    "version": "0.0",

    "browser_action": 
    {
        "default_icon": "icon.png",
        "default_popup": "renren.html"
    },

    "content_scripts": [{"matches": ["http://*.renren.com/*"],"js": ["tempbackground.js"]}],

    "permissions": [
        "cookies","http://*.renren.com/*"
        ]
}
